<Window x:Class="Flask.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flask"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        Background="{DynamicResource MaterialDesignPaper}"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        Title="Flask" Height="800" Width="800" WindowStyle="None" ResizeMode="NoResize" MouseLeftButtonDown="Window_MouseLeftButtonDown" Loaded="Window_Loaded">
    <Grid x:Name="MainGrid">
        <Grid x:Name="LevelCompleteStack" Visibility="Collapsed">
            <TextBlock Text="Perfect!" HorizontalAlignment="Center" FontSize="50" VerticalAlignment="Center" FontFamily="Ethnocentric"/>
            <StackPanel Margin="20,450,20,0">
                <Button x:Name="Next" Content="Next" Width="200" Height="50" FontSize="25" Margin="20" FontFamily="Ethnocentric" Click="Next_Click"/>
            </StackPanel>
        </Grid>
        
        <StackPanel x:Name="ExitMessage" Visibility="Collapsed" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Введите ваше имя для сохраниения на таблицу лидеров: " FontFamily="Comfortaa" HorizontalAlignment="Center" Margin="5"/>
            <TextBox x:Name="NamePlayer" Width="350" materialDesign:HintAssist.Hint="Ваше имя" FontFamily="Comfortaa" Margin="5"/>
            <Label x:Name="ErrorLabel" FontFamily="Comfortaa" Content="" HorizontalAlignment="Center" FontStyle="Italic" Foreground="#DDFF0000"/>
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                <Button x:Name="ExitSaveButton" Content="Сохранить и выйти" Margin="5" FontFamily="Comfortaa" Click="ExitSaveButton_Click"/>
                <Button x:Name="ExitButton" Content="Выйти" Margin="5" FontFamily="Comfortaa" Click="ExitButton_Click"/>
                <Button x:Name="CancelButton" Content="Отмена" Margin="5" FontFamily="Comfortaa" Click="Cancel_Click"/>
            </StackPanel>
        </StackPanel>

        <StackPanel x:Name="StartStack" Visibility="Visible" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="StartSave" Margin="10" Content="Продолжить предыдущую игру игру" Click="StartSave_Click"/>
            <Button x:Name="StartNew" Margin="10" Content="Начать новую игру" Click="StartNew_Click"/>
        </StackPanel>

        <Grid x:Name="PlayGrid">
            <StackPanel x:Name="Header">
                <TextBlock Text="Flask" FontSize="70" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="Ethnocentric" Margin="0,120,0,0"/>
                <TextBlock x:Name="LevelText" Text="Level 1" VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Ethnocentric" FontSize="38" Visibility="Collapsed"/>
                <StackPanel x:Name="ScoreStack" Orientation="Horizontal" HorizontalAlignment="Center" Margin="10" Visibility="Collapsed">
                    <TextBlock Text="Score: " FontSize="20" FontFamily="Ethnocentric"/>
                    <TextBlock x:Name="Score" FontSize="20" FontFamily="Ethnocentric" Text="{Binding Path=score}"/>
                </StackPanel>
            </StackPanel>
            
            <StackPanel x:Name="LeftButtons" Orientation="Horizontal" Margin="10">
                <Button x:Name="Reset" Width="70" Height="70" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5" Click="Reset_Click">
                    <Button.ToolTip>
                        <ToolTip>Перезагрузить уровень</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\reset.png" Margin="-16"/>
                </Button>
                <Button x:Name="Help" Width="70" Height="70" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5" Click="Help_Click">
                    <Button.ToolTip>
                        <ToolTip>Помощь</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\help.png" Margin="-12"/>
                </Button>
                <Button x:Name="BackHelp" Width="70" Height="70" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5" Click="BackHelp_Click" Visibility="Collapsed">
                    <Button.ToolTip>
                        <ToolTip>Назад</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\back.png" Margin="-12"/>
                </Button>
                <Button x:Name="AddFlask" Width="70" Height="70" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5" Click="Add_Click">
                    <Button.ToolTip>
                        <ToolTip>Добавить еще одну колбу (-10 очков)</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\flask.png" Margin="-12"/>
                </Button>
                <Button x:Name="LeaderBoardButton" Width="70" Height="70" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5" Click="LeaderBoardButton_Click">
                    <Button.ToolTip>
                        <ToolTip>Таблица лидеров</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\leaderBoard.png" Margin="-12"/>
                </Button>
                <Button x:Name="LeaderBoardButtonBack" Width="70" Height="70" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5" Click="LeaderBoardButtonBack_Click" Visibility="Collapsed">
                    <Button.ToolTip>
                        <ToolTip>Вернуться</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\leaderBoard.png" Margin="-12"/>
                </Button>
            </StackPanel>

            <StackPanel x:Name="RightButtons" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10" Orientation="Horizontal">
                <Button x:Name="Sver" Width="70" Height="70" Click="Sver_Click" Padding="0" Margin="5">
                    <Button.ToolTip>
                        <ToolTip>Свернуть</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\Sver.png" Margin="-6"/>
                </Button>
                <Button x:Name="Exit" Width="70" Height="70" Click="Exit_Click" Padding="0" Margin="5">
                    <Button.ToolTip>
                        <ToolTip>Закрыть</ToolTip>
                    </Button.ToolTip>
                    <Image Source="icons\exit.png" Margin="5"/>
                </Button>
            </StackPanel>

            <StackPanel x:Name="LeaderBoardStack" Margin="20,100,20,0" Visibility="Collapsed">
                <TextBlock x:Name="LeaderBoardText" Text="&#10;" FontSize="40" FontFamily="Consolas" TextWrapping="WrapWithOverflow"/>
            </StackPanel>

            <StackPanel x:Name="HelpPanel" Margin="20,100,20,0" Visibility="Collapsed">
                <TextBlock Text="   Помощь и правила игры:
                           1. Выберите колбу, нажав на нее.
                           2. Выберите вторую колбу.
                           3. Ячейка 'Перельется' в новую.
                           4. Переливать можно только в такие же ячейки.
                           5. Уровень будет завершен тогда, когда каждый из цветов будет собран в своей колбе.
                           В начале уровня Вам дается определенное количество очков, но за каждый ваш ход будет отниматься одно из них. Ваша задача сделать минимальное количество ходов для максимального количества очков." FontSize="40" FontFamily="Consolas" TextWrapping="WrapWithOverflow"/>
            </StackPanel>

